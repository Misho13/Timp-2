# Timp-2
#include "stdafx.h"
#include <stdlib.h>
#include <iostream>
#include <string.h>

using namespace std;
#define MAXBUF 1024
int _tmain(int argc, _TCHAR* argv[])
{
	char text[MAXBUF];
	char *r, *r1;
	FILE *f1;
	f1 = fopen("1.txt", "r");
	int i = 0;
	fread(text, sizeof(char), MAXBUF, f1);
	r = text;
	do
	{
		if (strstr(r, "/**") != nullptr)
		{
			r = strstr(r, "func");
			r += 4;
			i = 0;
			r1 = strstr(r, "*/");
			do{
				cout << r[i];
				r++;
			} while (r != r1);
			cout << "\n";
		}
		else break;
	} while (r != nullptr); 
	system("pause");
	return 0;
}
